{
    "name": "remark-mdx-metadata",
    "description": "Remark transformer for modifying MDX metadata.",
    "version": "1.0.0-beta.2",
    "author": {
        "name": "Michael Novotny",
        "email": "manovotny@gmail.com",
        "url": "https://manovotny.com"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/manovotny/remark-mdx-metadata.git"
    },
    "keywords": [
        "markdown",
        "mdx",
        "mdx-js",
        "meta",
        "metadata",
        "remark",
        "remark-mdx",
        "remark-plugin"
    ],
    "engines": {
        "node": ">=8.6.0",
        "yarn": ">=1.0.0"
    },
    "main": "index.js",
    "scripts": {
        "coverage": "jest --coverage",
        "example": "node example/example.js",
        "lint": "eslint . --ext .json --ext .js",
        "prettier": "prettier --write \"**/*.{js,json,md,mdx,yml}\"",
        "test": "yarn lint && yarn coverage",
        "unit": "jest"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,json,md,mdx,yml}": [
            "prettier --write",
            "git add"
        ]
    },
    "dependencies": {
        "@babel/generator": "7.3.2",
        "@babel/parser": "7.3.2",
        "@babel/traverse": "7.2.3",
        "stringify-object": "3.3.0"
    },
    "devDependencies": {
        "eslint": "4.19.1",
        "eslint-config-get-off-my-lawn-prettier": "1.0.0",
        "husky": "1.3.1",
        "jest": "24.1.0",
        "jest-snapshot-serializer-raw": "1.1.0",
        "lint-staged": "8.1.3",
        "prettier": "1.16.4",
        "remark": "10.0.1",
        "remark-mdx": "0.17.2",
        "remark-stringify": "6.0.4",
        "to-vfile": "5.0.2"
    },
    "peerDependencies": {
        "remark-mdx": "^0.17.2"
    }
}
